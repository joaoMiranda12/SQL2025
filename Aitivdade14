CREATE DATABASE canada;
Use canada;

CREATE TABLE cidades(
CidadeID INT PRIMARY KEY,
Nome_cidade VARCHAR(100) NOT NULL,
Codigo_Postal INT NULL
);

SELECT * FROM Cidades;
SELECT CidadeID FROM  Cidades;

